<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Haml">
            <file name="file.haml" url="HamlTemplate.vala">
                <attr name="displayName" bundlevalue="org.mephi.haml.Bundle#Templates/Haml/HamlTemplate.haml"/>
                <attr name="template" boolvalue="true"/>
            </file>
        </folder>    
    </folder>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-haml">
		<file name="org-netbeans-modules-editor-NbEditorKit.instance"/>
		<file name="language.instance">
                    <attr name="instanceCreate" methodvalue="org.mephi.haml.lexer.HamlTokenId.getLanguage"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
		</file>
		<attr name="SystemFileSystem.localizingBundle" stringvalue="org.mephi.haml.Bundle"/>
		<folder name="FontsColors">
                    <folder name="NetBeans">
			<folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
				<attr name="SystemFileSystem.localizingBundle" stringvalue="org.mephi.haml.Bundle"/>
                            </file>
			</folder>
                    </folder>
		</folder>
            </folder>
	</folder>
    </folder>
</filesystem>